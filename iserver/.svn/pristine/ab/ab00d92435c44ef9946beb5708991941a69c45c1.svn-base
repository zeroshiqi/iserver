package cn.ichazuo.commons.authorize;

import java.io.Serializable;

/**
 * @ClassName: AuthorizeButton
 * @Description: (权限按钮)
 * @author ZhaoXu
 * @date 2015年6月28日 下午4:24:39
 * @version V1.0
 */
public class AuthorizeButton implements Serializable {
	private static final long serialVersionUID = 1L;
	private String id; // id
	private String name; // name
	private String icon; // 图标
	private String function;// click方法
	private AuthorizeProperties proper; // 所属资源

	/* get set */
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	public String getFunction() {
		return function;
	}

	public void setFunction(String function) {
		this.function = function;
	}

	public AuthorizeProperties getProper() {
		return proper;
	}

	public void setProper(AuthorizeProperties proper) {
		this.proper = proper;
	}
}
