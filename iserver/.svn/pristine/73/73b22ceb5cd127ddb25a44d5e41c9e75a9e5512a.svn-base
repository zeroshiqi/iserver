<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.ichazuo.dao.CityDao">
	
	<!-- 查询全部城市 -->
	<select id="findAllCity" resultType="cn.ichazuo.model.Dictionary">
		select id,city_name as value from t_city where 1=1 and status = 1
	</select>
</mapper>