package cn.ichazuo.model.table;

import java.util.Date;

import javax.persistence.Table;

import cn.ichazuo.commons.base.BaseModel;

/**
 * @ClassName: CourseWebCrowdfunding
 * @Description: (众筹信息)
 * @author ZhaoXu
 * @date 2015年9月14日 下午1:49:52
 * @version V1.0
 */
@Table(name = "t_course_web_crowdfunding")
public class CourseWebCrowdfunding extends BaseModel {
	private static final long serialVersionUID = 1L;
	private Long id; // id
	private Long courseId; // 课程ID
	private Integer number; // 数量
	private Double price; // 价格
	private String content; // 内容
	private String uuid; // 随机字符串
	private String unionId; // 唯一ID
	private String nickname; // 姓名
	private Date createAt; // 创建时间
	private Date updateAt; // 修改时间
	private String avatar; // 头像
	private Integer status; // 状态
	private Long version; // 版本
	private Integer felicitate; // 祝贺数量
	private String weixinSex; // 微信性别
	private Integer refund;	//退款
	private String weixinInfo;	//微信号
	private String phone;	//手机号

	// get set
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getCourseId() {
		return courseId;
	}

	public void setCourseId(Long courseId) {
		this.courseId = courseId;
	}

	public Integer getNumber() {
		return number;
	}

	public void setNumber(Integer number) {
		this.number = number;
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getUuid() {
		return uuid;
	}

	public void setUuid(String uuid) {
		this.uuid = uuid;
	}

	public String getUnionId() {
		return unionId;
	}

	public void setUnionId(String unionId) {
		this.unionId = unionId;
	}

	public Date getCreateAt() {
		return createAt;
	}

	public void setCreateAt(Date createAt) {
		this.createAt = createAt;
	}

	public Date getUpdateAt() {
		return updateAt;
	}

	public void setUpdateAt(Date updateAt) {
		this.updateAt = updateAt;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Long getVersion() {
		return version;
	}

	public void setVersion(Long version) {
		this.version = version;
	}

	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getAvatar() {
		return avatar;
	}

	public void setAvatar(String avatar) {
		this.avatar = avatar;
	}

	public Integer getFelicitate() {
		return felicitate;
	}

	public void setFelicitate(Integer felicitate) {
		this.felicitate = felicitate;
	}

	public String getWeixinSex() {
		return weixinSex;
	}

	public void setWeixinSex(String weixinSex) {
		this.weixinSex = weixinSex;
	}

	public Integer getRefund() {
		return refund;
	}

	public void setRefund(Integer refund) {
		this.refund = refund;
	}

	public String getWeixinInfo() {
		return weixinInfo;
	}

	public void setWeixinInfo(String weixinInfo) {
		this.weixinInfo = weixinInfo;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}
